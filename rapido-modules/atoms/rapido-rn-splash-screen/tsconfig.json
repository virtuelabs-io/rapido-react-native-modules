{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"module": "esnext",
		"outDir": "../build/dist/rapido-rn-splash-screen",
		"experimentalDecorators": true,
		"sourceMap": true,
		"jsx": "react-native",
		"lib": ["dom", "esnext"],
		"moduleResolution": "node",
		"noEmit": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"strict": true,
		"noEmitHelpers": true,
		"importHelpers": true,
		"noImplicitReturns": true,
		"declaration": true
	}
}
